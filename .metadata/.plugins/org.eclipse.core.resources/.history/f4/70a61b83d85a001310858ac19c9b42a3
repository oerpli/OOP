public class Lager {
	Lagerplatz[] lp = new Lagerplatz[4];

	public Lager(int L_30, int L_18, int L5, int L20) {
		this.lp[0] = new LP30(L_30);
		this.lp[1] = new LP18(L_18);
		this.lp[2] = new LP5(L5);
		this.lp[3] = new LP20(L20);
	}

	protected int store(W5 w) {
		if (lp[2].store(w)) {
			return 1;
		} else
			return -1;
	}

	protected int store(W20 w) {
		if (lp[3].store(w)) {
			return 1;
		} else if (lp[2].store(w)) {
		}
	}

	private int store(int i, Ware w) {

	}
}
