class Lagerplatz {
	private Ware[] waren;

	Lagerplatz(int size) {
		waren = new Ware[size];
	}

	protected boolean store(Ware w) {
		for (int i = 0; i < waren.length; i++) {
			if (waren[i] == null) {
				waren[i] = w;
				return true;
			}
		}
		return false;
	}

	public Ware remove(int nr) {
		for (int i = 0; i < waren.length; i++) {
			if (waren[i].number == nr) {
				Ware x = waren[i];
				waren[i] = null;
				return x;
			}
		}
		return null;
	}

	public int[] util() {
		int[] x = { 0, waren.length };
		for (Ware w : waren) {
			if (w != null) {
				x[0] += 1;
			}
		}
	}

	public String toString() {
		String out = "";
		for (Ware w : waren) {
			if (w != null)
				out += w.number + '\t' + w.name + '\n';
		}
		return out;
	}
}
