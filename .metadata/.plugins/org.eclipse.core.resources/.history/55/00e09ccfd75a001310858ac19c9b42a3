public class Lager {
	Lagerplatz[] lp = new Lagerplatz[4];

	public Lager(int L_30, int L_18, int L5, int L20) {
		this.lp[0] = new LP30(L_30);
		this.lp[1] = new Lagerplatz[L_18];
		this.lp[2] = new Lagerplatz[L5];
		this.lp[3] = new Lagerplatz[L20];
	}

	protected int store(W5 w) {
		return this.store(2, w);
	}

	private int store(int i, Ware w) {
		for (Lagerplatz p : lp[i]) {
			if (p == null) {
				p = w;
				return i;
			}
		}
	}
}
